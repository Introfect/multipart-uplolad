{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cf-typegen": "wrangler types --env dev",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:deploy": "dotenv -e .env -- drizzle-kit migrate",
    "db:start": "supabase start",
    "db:reset": "dotenv -e .env -- supabase db reset",
    "dev": "wrangler dev --env dev",
    "deploy:production": "wrangler deploy --env production",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/zod-openapi": "1.2.0",
    "@scalar/hono-api-reference": "^0.9.30",
    "@tsndr/cloudflare-worker-jwt": "^3.2.1",
    "bcryptjs": "^3.0.3",
    "drizzle-orm": "^0.45.1",
    "hono": "4.11.1",
    "postgres": "^3.4.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251223.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "25.0.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "supabase": "^2.70.1",
    "typescript": "^5.9.3",
    "wrangler": "^4.56.0"
  }
}
